{
  "name": "template-next",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "tsc && vite build",
    "build:aliyun-dev": "tsc && vite build --mode development",
    "build:aliyun-staging": "tsc && vite build --mode staging",
    "build:aliyun-prod": "tsc && vite build --mode production",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives --max-warnings 999",
    "lint-fix": "eslint src --fix --ext .js,.jsx,.ts,.tsx",
    "postinithook": "husky add .husky/pre-commit \"npx lint-staged\"",
    "inithook": "husky install",
    "preview": "npm run build:aliyun-prod && vite preview",
    "mock": "json-server --watch mock/db.json"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "@fortaine/fetch-event-source": "^3.0.6",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "js-cookie": "^3.0.5",
    "qs": "^6.11.2",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.4.4",
    "react-markdown": "^8.0.5",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.13.0",
    "react-syntax-highlighter": "^15.5.0",
    "rehype-katex": "^6.0.2",
    "remark-breaks": "^3.0.2",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "sanitize-html": "^2.11.0",
    "sea-lion-ui": "^0.0.45",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.3",
    "@types/node": "^20.3.1",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@emoji-mart/data": "^1.1.2",
    "@emoji-mart/react": "^1.1.1",
    "eslint": "^8.38.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "husky": "^8.0.3",
    "json-server": "^0.17.3",
    "less": "^4.1.3",
    "lint-staged": "^13.2.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-plugin-cdn-import": "^0.3.5"
  }
}
